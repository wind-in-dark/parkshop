<div id="page-wrapper">
    <div class="row">
    <div class="col-lg-12">
        <h1>工具</h1>
        <ol class="breadcrumb">
            <li><a href="/admin/"><i class="fa fa-dashboard"></i>系统工具</a></li>
            <li class="active"><i class="fa fa-table"></i>二维码生成和解析</li>
        </ol>
    </div>
</div>
<center>
<div class="row">
    <div class="col-lg-12">
        <div class="table-responsive">
            <table class="table table-hover table-bordered">
                <thead>
                <tr>
                    <th>我的二维码</th>
                    <th>生成二维码</th>
                    <th>解析二维码</th>
                    </tr>
                </thead>
                <tbody>
                <tr>
					<td>
						
						<div class="dropdown">
							<button type="button" class="btn dropdown-toggle" id="dropdownMenu1" 
								data-toggle="dropdown">
								#if($curDimensionType)
									$!{curDimensionType}
								#else
								简历二维码
								#end
								<span class="caret"></span>
							</button>
							<ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu1">
								<li role="presentation">
									<a role="menuitem" tabindex="-1" href="/tools/twoDimension/mainPage">主页二维码</a>
								</li>
								<li role="presentation">
									<a role="menuitem" tabindex="-1" href="/tools/twoDimension/blog">博客列表二维码</a>
								</li>
								<li role="presentation">
									<a role="menuitem" tabindex="-1" href="/tools/twoDimension/project">项目二维码</a>
								</li>
								<li role="presentation">
									<a role="menuitem" tabindex="-1" href="/tools/twoDimension/resume">简历二维码</a>
								</li>
							</ul>
						</div>


					</td>
                    <td>
						<form action="/tools/twoDimension/textToImage" method="post">
						
                                        <textarea name="twoDimensionData" class="col-lg-12" rows="6" id="twoDimensionDataId" placeholder="请输入您要转换为二维码的xinxi" required="true">	</textarea>							
										<br/>
										<button class="btn btn-success btn-mini"  id="twoDimensionDataButton">
                                        	<i class="icon-print">生成二维码</i>
										</button>
							
                        </form> 
					</td>
					
                    <td><form action="/tools/twoDimension/imageToText" method="post" enctype="multipart/form-data">
                    
						<div>
						<div style="float: left;" id="tipsTwo"><input type="file"  name="TP_URL" id="uploadify1" keepDefaultStyle = "true"/></div>
							<div><input class="btn btn-success" type="submit" value="解析"/></div>
						</div>
						</form>
					</td>
                </tr>
				<tr>
					 <td>
					<div id="myImageShowAreaId">
						#if($myTwoDimension)
						<img src="$!{myTwoDimension}" class="img-thumbnail" width="200" height="200">
					
						#end
                    </div>
					</td>
                    <td>
					<div id="imageShowAreaId">
						#if($picUrl)
						<img src="$!{picUrl}" class="img-thumbnail" width="200" height="200">
					
						#end
                    </div>
					</td>
                    <td>
						<div>
								<textarea id="showImageContentAreaId" class="col-lg-12" rows="6" title="解析结果" placeholder="显示解析结果">
								$!{picData}
								</textarea>
						</div>
					</td>
                </tr>
                </tbody>
                
            </table>
        </div>

    </div>
</div><!-- /.row -->
</div><!-- /#page-wrapper -->
</center>
</div><!-- /#wrapper -->

<!-- JavaScript -->

<script src="//cdn.bootcss.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
<script src="//cdn.bootcss.com/bootbox.js/4.4.0/bootbox.min.js"></script>
<script src="/js/jquery.confirm.min.js"></script>
<script src="/js/jcala-form.min.js"></script>
</body>
</html>