#parse("/templates/module/header.vm")
<div class="container">
    <div class="row">
        <div class="col-lg-12 post">
            <div class="editormd-preview" id="md-container">
                <div class="markdown-body editormd-preview-container" previewcontainer="true" style="padding: 20px;">
                #foreach($item in $!{projects})
                <h2>
                    <a class="reference-link"></a>
                    <span class="header-link octicon octicon-link"></span>
                    <a href="$!{item.Url}" title="careerBlog">$!{item.Name}</a></h2>
                <ul>
                    <li>
                        <strong>创建日期&nbsp;:&nbsp;</strong>$!{item.Date}</li>
                    <li>
                        <strong>所用技术&nbsp;:&nbsp;</strong>$!{item.Tech}</li>
                    <li>
                        <strong>项目描述&nbsp;:</strong>
                        <blockquote>
                            <p>$!{item.Desp}</p>
                        </blockquote>
                    </li>
                </ul>
                <br><br>
            #end
                </div>
            </div>
			#if($current)
				#set($href = "/projects")
				#frontFlip($!{pageNum} $!{current} $href)
			#end
				</div>
    </div>
</div>
#include("/templates/module/footer.vm")